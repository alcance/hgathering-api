{
  "name": "acopio",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "legacy_id": {
      "type": "string"
    },
    "nombre": {
      "type": "string"
    },
    "direccion": {
      "type": "string"
    },
    "geopos": {
      "type": "geopoint"
    },
    "comments": {
      "type": "string"
    }
  },
  "validations": [],
  "relations": {
    "contactos": {
      "type": "hasMany",
      "model": "contacto"
    },
    "productos": {
      "type": "hasMany",
      "model": "producto"
    }
  },
  "acls": [
    {
      "accessType": "WRITE",
      "principalType": "ROLE",
      "principalId": "$unauthenticated",
      "permission": "DENY"
    },
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "admin",
      "permission": "ALLOW"
    },
    {
      "accessType": "WRITE",
      "principalType": "ROLE",
      "principalId": "$authenticated",
      "permission": "ALLOW"
    }
  ],
  "methods": {}
}
