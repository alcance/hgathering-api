{
  "name": "acopio",
  "plural": "acopios",
  "base": "lugar",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "legacy_id": {
      "type": "string"
    },
    "nombre": {
      "type": "string"
    },
    "details": {
      "type": "string"
    },
    "estado": {
      "type": "string"
    },
    "fechaDeActualizacion": {
      "type": "date",
      "required": true,
      "defaultFn": "now"
    }
  },
  "validations": [],
  "relations": {
    "aceptan": {
      "type": "referencesMany",
      "model": "producto"
    },
    "contactos": {
      "type": "embedsMany",
      "model": "contacto",
      "property": "contactos",
      "options": {
        "validate": true,
        "forceId": false
      }
    },
    "comentarios": {
      "type": "embedsMany",
      "model": "comentario",
      "property": "comentarios",
      "options": {
        "validate": true,
        "forceId": false
      }
    },
    "existencias": {
      "type": "hasMany",
      "model": "existencia_acopio",
      "foreignKey": ""
    }
  },
  "acls": [
    {
      "accessType": "WRITE",
      "principalType": "ROLE",
      "principalId": "$unauthenticated",
      "permission": "DENY"
    },
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "admin",
      "permission": "ALLOW"
    },
    {
      "accessType": "WRITE",
      "principalType": "ROLE",
      "principalId": "$authenticated",
      "permission": "ALLOW"
    }
  ],
  "methods": {}
}
